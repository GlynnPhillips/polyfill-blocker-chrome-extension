{
  "manifest_version": 3,
  "name": "Polyfill.io Blocker",
  "description": "Block requests to polyfill.io",
  "version": "1.0",
  "permissions": [
    "declarativeNetRequest"
  ],
   "declarative_net_request" : {
        "rule_resources" : [
            {
                "id" : "ruleset_1",
                "enabled": true,
                "path": "rules_1.json"
            }
        ]
    }
}